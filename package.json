{
  "name": "electron-url-launcher",
  "version": "1.0.0",
  "description": "Electron URL Launcher",
  "main": "index.js",
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icns/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-url-launcher --overwrite --asar --platform=win32 --arch=ia32,x64 --icon=assets/icons/ico/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron URL Launcher\"",
    "package-linux": "electron-packager . electron-url-launcher --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bert-huang/electron-url-launcher.git"
  },
  "keywords": [
    "electron",
    "url",
    "window"
  ],
  "author": "bert-huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bert-huang/electron-url-launcher/issues"
  },
  "homepage": "https://github.com/bert-huang/electron-url-launcher#readme",
  "dependencies": {
    "chokidar": "^2.0.3",
    "find-process": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.1.2",
    "rimraf": "^2.6.2",
    "url-parse": "^1.4.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-packager": "^12.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1"
  }
}
